{
  "name": "resium",
  "version": "1.14.3",
  "description": "React components for Cesium",
  "source": "./src/index.ts",
  "main": "./dist/resium.umd.cjs",
  "module": "./dist/resium.mjs",
  "exports": {
    ".": {
      "import": "./dist/resium.mjs",
      "require": "./dist/resium.umd.cjs"
    }
  },
  "unpkg": "./dist/resium.umd.cjs",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/reearth/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type": "tsc",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o docs/static/examples",
    "docs": "yarn --cwd docs start",
    "docs:build": "yarn --cwd docs generate && build-storybook -s .storybook/public -o docs/static/examples && yarn --cwd docs build",
    "docs:generate": "yarn --cwd docs generate"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-storysource": "6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-vite": "^0.2.0",
    "@storybook/react": "6.5.9",
    "@storybook/theming": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.15",
    "c8": "^7.11.3",
    "cesium": "1.95.0",
    "eslint": "^8.19.0",
    "eslint-config-reearth": "https://github.com/reearth/eslint-config-reearth.git#d1f1466",
    "jsdom": "^20.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-expect": "^1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-cesium": "1.2.19",
    "vite-plugin-dts": "^1.3.0",
    "vitest": "^0.18.0"
  }
}
