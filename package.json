{
  "name": "resium",
  "version": "1.18.3",
  "description": "React component library for Cesium",
  "source": "./src/index.ts",
  "main": "./dist/resium.umd.js",
  "module": "./dist/resium.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/resium.mjs",
      "require": "./dist/resium.umd.js"
    }
  },
  "repository": "https://github.com/reearth/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type": "tsc",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "storybook": "storybook dev -p 9001",
    "storybook:build": "storybook build -o docs/static/examples",
    "docs": "yarn --cwd docs start",
    "docs:build": "run-s docs:generate storybook:build docs:build2",
    "docs:generate": "ts-node --esm -O '{\"module\":\"ESNext\"}' ./scripts/generator/index.mts",
    "docs:build2": "mv docs/static/examples/cesium docs/static/ && yarn --cwd docs build"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=20.11.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=18.2.0 <20",
    "react-dom": ">=18.2.0 <20"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-storysource": "8.5.3",
    "@storybook/addons": "7.6.20",
    "@storybook/builder-vite": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/react-vite": "8.5.3",
    "@storybook/theming": "8.5.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "4.3.4",
    "c8": "10.1.3",
    "cesium": "1.126.0",
    "eslint": "8.57.0",
    "eslint-config-reearth": "0.3.0",
    "globby": "14.0.1",
    "jsdom": "26.0.0",
    "npm-run-all2": "7.0.2",
    "prettier": "3.2.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "storybook": "8.5.3",
    "ts-expect": "1.3.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.3",
    "vite": "6.1.0",
    "vite-plugin-cesium": "1.2.23",
    "vite-plugin-dts": "4.5.0",
    "vitest": "3.0.5",
    "web-streams-polyfill": "4.0.0"
  },
  "packageManager": "yarn@4.6.0"
}
