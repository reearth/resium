{
  "name": "resium",
  "version": "1.19.2",
  "type": "module",
  "description": "React component library for Cesium",
  "source": "./src/index.ts",
  "main": "./dist/resium.umd.cjs",
  "module": "./dist/resium.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/resium.js",
      "require": "./dist/resium.umd.cjs"
    }
  },
  "repository": "https://github.com/reearth/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type": "tsc",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "storybook": "storybook dev -p 9001",
    "storybook:build": "storybook build -o docs/static/examples && cp -r node_modules/cesium/Build/Cesium docs/static/examples/cesium",
    "docs": "yarn --cwd docs start",
    "docs:build": "run-s build docs:generate storybook:build docs:build2",
    "docs:generate": "ts-node --esm -O '{\"module\":\"ESNext\"}' ./scripts/generator/index.mts",
    "docs:build2": "yarn --cwd docs build"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=20.11.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "resolutions": {
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "@types/react": "^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/builder-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "4.2.1",
    "c8": "10.1.3",
    "cesium": "^1.137.0",
    "eslint": "9.39.2",
    "eslint-config-reearth": "0.3.8",
    "globby": "16.1.0",
    "jsdom": "27.4.0",
    "npm-run-all2": "8.0.4",
    "prettier": "^3.8.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^8.6.14",
    "ts-expect": "1.3.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite": "5.2.7",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-dts": "4.5.4",
    "vitest": "1.4.0",
    "web-streams-polyfill": "^4.2.0"
  }
}
