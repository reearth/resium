{
  "name": "resium",
  "version": "1.18.4",
  "description": "React component library for Cesium",
  "source": "./src/index.ts",
  "main": "./dist/resium.umd.js",
  "module": "./dist/resium.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/resium.mjs",
      "require": "./dist/resium.umd.js"
    }
  },
  "repository": "https://github.com/reearth/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type": "tsc",
    "lint": "eslint .",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 9001",
    "storybook:build": "storybook build -o docs/static/examples",
    "docs": "yarn --cwd docs start",
    "docs:build": "run-s build docs:generate storybook:build docs:build2",
    "docs:generate": "ts-node --esm -O '{\"module\":\"ESNext\"}' ./scripts/generator/index.mts",
    "docs:build2": "mv docs/static/examples/cesium docs/static/ && yarn --cwd docs build"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=22.21.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  },
  "resolutions": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@types/react": "^19.2.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/builder-vite": "^9.1.15",
    "@storybook/react": "^9.1.15",
    "@storybook/react-vite": "^9.1.15",
    "@storybook/theming": "^8.6.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "c8": "10.1.3",
    "cesium": "1.116.0",
    "eslint": "9.38.0",
    "eslint-config-reearth": "0.3.8",
    "globby": "15.0.0",
    "jsdom": "27.0.1",
    "npm-run-all2": "8.0.4",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.15",
    "ts-expect": "1.3.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vite-plugin-cesium": "1.2.23",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.4",
    "web-streams-polyfill": "4.2.0"
  }
}
